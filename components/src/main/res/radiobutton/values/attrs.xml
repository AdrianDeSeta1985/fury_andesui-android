<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="AndesRadiobutton" tools:ignore="ResourceName">

        <attr name="andesRadiobuttonAlign" format="enum">
            <enum name="left" value="1000" />
            <enum name="right" value="1001" />
        </attr>

        <attr name="andesRadiobuttonStatus" format="enum">
            <enum name="selected" value="2000" />
            <enum name="unselected" value="2001" />
        </attr>

        <attr name="andesRadiobuttonType" format="enum">
            <enum name="idle" value="3000" />
            <enum name="disabled" value="3001" />
            <enum name="error" value="3002" />
        </attr>

        <attr name="andesRadiobuttonText" format="reference" />

    </declare-styleable>

</resources>