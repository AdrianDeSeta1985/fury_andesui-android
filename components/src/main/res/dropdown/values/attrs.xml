<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="AndesDropdown" tools:ignore="ResourceName">

        <attr name="andesDropdownLabel" format="reference" />

        <attr name="andesDropdownHelper" format="reference" />

        <attr name="andesDropdownPlaceholder" format="reference" />

        <attr name="andesDropdownState">
            <enum name="enabled" value="20" />
            <enum name="error" value="21" />
            <enum name="disabled" value="22" />
            <enum name="readonly" value="23" />
        </attr>
    </declare-styleable>
</resources>